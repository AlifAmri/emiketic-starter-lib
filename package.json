{
  "private": true,
  "name": "starter-lib",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js ./",
    "lint:critical": "eslint --no-eslintrc --config .eslintrc.critical.json ./",
    "format": "npm run format:all ; npm run format:js",
    "format:all": "prettier ./**/*.{md,json,js,css,scss} --write",
    "format:js": "eslint --ext .js --fix ./ ; exit 0",
    "test": "jest",
    "build": "babel --source-maps true ./src/ -d ./dist/",
    "build:watch": "babel -w --source-maps true ./src/ -d ./dist/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "debug": "~4.1.0",
    "events": "~3.0.0",
    "formdata-polyfill": "~3.0.12",
    "whatwg-fetch": "~3.0.0"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-eslint": "~10.0.1",
    "babel-jest": "~23.6.0",
    "babel-preset-env": "~1.7.0",
    "babel-preset-stage-2": "~6.24.1",
    "eslint": "~5.8.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-plugin-import": "~2.14.0",
    "intl": "~1.2.5",
    "intl-locales-supported": "~1.0.0",
    "jest": "~23.6.0",
    "prettier": "~1.14.3"
  }
}
